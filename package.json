{
  "name": "agentbootup",
  "version": "0.5.0",
  "description": "Seed Claude Code, OpenAI Codex, Gemini CLI, and Windsurf development assets (agents, skills, commands, workflows) into any project.",
  "type": "module",
  "bin": {
    "agentbootup": "bootup.mjs"
  },
  "files": [
    "bootup.mjs",
    "templates/**",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "keywords": [
    "claude",
    "claude-code",
    "codex",
    "gemini",
    "gemini-cli",
    "windsurf",
    "bootstrap",
    "agents",
    "skills",
    "workflows"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dundas/agentbootup.git"
  },
  "author": "",
  "scripts": {
    "dry-run": "node ./bootup.mjs --dry-run --verbose",
    "sync-templates": "node ./scripts/sync-templates.mjs",
    "check-templates": "node ./scripts/sync-templates.mjs --check",
    "test": "node -e 'console.log(\"ok\")'"
  }
}
